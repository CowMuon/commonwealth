web: cd packages/commonwealth && NODE_OPTIONS=--max-old-space-size=$WEB_MAX_OLD_SPACE_SIZE ts-node -P tsconfig.server.json -T server.ts
consumer: cd packages/commonwealth && node --max-old-space-size=$CONSUMER_MAX_OLD_SPACE_SIZE build/commonwealth/server/CommonwealthConsumer/CommonwealthConsumer.js run-as-script
release: cd packages/commonwealth && npx sequelize-cli db:migrate --config server/sequelize.json
